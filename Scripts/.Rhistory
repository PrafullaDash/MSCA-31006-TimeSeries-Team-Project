autolayer(Model_Mean$mean, series="Mean") +
autolayer(Model_Naive$mean, series="Naïve") +
autolayer(Model_Drift$mean, series="Drift") +
ggtitle("Forecasts for daily Google Wikepedia Page") +
xlab("Days") + ylab("Google traffic")
